    package="com.takatutustudio.Donasi">

    <dist:module dist:instant="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".KonfirmasiActivity" />
        <activity android:name=".MetodePembayaranActivity" />
        <activity android:name=".ShodaqohActivity" />
        <activity android:name=".SuccessDonateActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".SuccessRegisterActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".GetStartedActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterOneActivity" />
    </application>

</manifest>